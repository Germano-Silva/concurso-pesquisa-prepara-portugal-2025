    "}\n",
    "\n",
    "def load_and_analyze_dataset(filepath, dataset_name, encoding='utf-8'):\n",
    "    \"\"\"\n",
    "    Carrega dataset e retorna an√°lise estrutural completa\n",
    "    \n",
    "    Args:\n",
    "        filepath: Caminho para o arquivo CSV\n",
    "        dataset_name: Nome descritivo do dataset\n",
    "        encoding: Codifica√ß√£o do arquivo\n",
    "    \n",
    "    Returns:\n",
    "        DataFrame carregado\n",
    "    \"\"\"\n",
    "    try:\n",
    "        df = pd.read_csv(filepath, encoding=encoding)\n",
    "        print(f\"\\n{'='*60}\")\n",
    "        print(f\"üìÅ DATASET: {dataset_name.upper()}\")\n",
    "        print(f\"üìÑ Arquivo: {filepath.name}\")\n",
    "        print(f\"{'='*60}\")\n",
    "        \n",
    "        return df\n",
    "        \n",
    "    except UnicodeDecodeError:\n",
    "        print(f\"‚ö†Ô∏è  Erro de encoding UTF-8, tentando ISO-8859-1...\")\n",
    "        try:\n",
    "            df = pd.read_csv(filepath, encoding='iso-8859-1')\n",
    "            print(f\"‚úÖ Carregado com ISO-8859-1\")\n",
    "            return df\n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Erro ao carregar {filepath}: {e}\")\n",
    "            return None\n",
    "    except Exception as e:\n",
    "        print(f\"‚ùå Erro ao carregar {filepath}: {e}\")\n",
    "        return None\n",
    "\n",
    "def analyze_missing_data(df, dataset_name):\n",
    "    \"\"\"\n",
    "    Analisa dados ausentes no dataset\n",
    "    \n",
    "    Args:\n",
    "        df: DataFrame para an√°lise\n",
    "        dataset_name: Nome do dataset\n",
    "    \n",
    "    Returns:\n",
    "        DataFrame com estat√≠sticas de missing data\n",
    "    \"\"\"\n",
    "    print(f\"\\nüîç AN√ÅLISE DE DADOS AUSENTES - {dataset_name}\")\n",
    "    print(\"-\" * 50)\n",
    "    \n",
    "    missing = df.isnull().sum()\n",
    "    missing_pct = (missing / len(df)) * 100\n",
    "    \n",
    "    missing_df = pd.DataFrame({\n",
    "        'Coluna': df.columns,\n",
    "        'Valores_Ausentes': missing.values,\n",
    "        'Percentual_Ausente': missing_pct.values\n",
    "    })\n",
    "    \n",
    "    missing_df = missing_df[missing_df['Valores_Ausentes'] > 0].sort_values(\n",
    "        'Percentual_Ausente', ascending=False\n",
    "    )\n",
    "    \n",
    "    if len(missing_df) == 0:\n",
    "        print(\"‚úÖ Nenhum valor ausente encontrado!\")\n",
    "    else:\n",
    "        print(f\"‚ö†Ô∏è  Encontrados valores ausentes em {len(missing_df)} colunas:\")\n",
    "        display(missing_df)\n",
    "    \n",
    "    return missing_df\n",
    "\n",
    "def create_summary_stats(df, categorical_cols, numerical_cols=[]):\n",
    "    \"\"\"\n",
    "    Cria estat√≠sticas resumo para colunas categ√≥ricas e num√©ricas\n",
    "    \n",
    "    Args:\n",
    "        df: DataFrame\n",
    "        categorical_cols: Lista de colunas categ√≥ricas\n",
    "        numerical_cols: Lista de colunas num√©ricas\n",
    "    \n",
    "    Returns:\n",
    "        Dicion√°rio com estat√≠sticas\n",
    "    \"\"\"\n",
    "    stats = {}\n",
    "    \n",
    "    # Estat√≠sticas categ√≥ricas\n",
    "    for col in categorical_cols:\n",
    "        if col in df.columns:\n",
    "            stats[col] = {\n",
    "                'valores_unicos': df[col].nunique(),\n",
    "                'valor_mais_frequente': df[col].mode().iloc[0] if len(df[col].mode()) > 0 else None,\n",
    "                'frequencia_maxima': df[col].value_counts().iloc[0] if len(df[col]) > 0 else 0,\n",
    "                'top_5_valores': df[col].value_counts().head().to_dict()\n",
    "            }\n",
    "    \n",
    "    # Estat√≠sticas num√©ricas\n",
    "    for col in numerical_cols:\n",
    "        if col in df.columns:\n",
    "            stats[col] = df[col].describe().to_dict()\n",
    "    \n",
    "    return stats\n",
    "\n",
    "print(\"‚úÖ Fun√ß√µes utilit√°rias definidas!\")"
   ]
