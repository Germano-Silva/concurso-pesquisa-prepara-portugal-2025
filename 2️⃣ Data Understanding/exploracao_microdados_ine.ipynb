    "\n",
    "# Dicion√°rio dos datasets principais\n",
    "DATASETS = {\n",
    "    'educacao_nacionalidade': POPULACAO_ESTRANGEIRA_PATH / \"Q2.1.csv\",\n",
    "    'setores_nacionalidade': POPULACAO_ESTRANGEIRA_PATH / \"Q3.3.csv\",\n",
