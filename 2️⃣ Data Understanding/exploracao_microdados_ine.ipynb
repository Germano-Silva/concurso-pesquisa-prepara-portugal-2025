   "source": [
    "### 5.1 An√°lise de Nacionalidades"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"üåç AN√ÅLISE DAS NACIONALIDADES\")\n",
    "print(\"=\" * 50)\n",
    "\n",
    "# Fun√ß√£o para extrair nacionalidades de qualquer dataset\n",
    "def extract_nacionalidades(df, dataset_name):\n",
    "    \"\"\"Extrai e analisa as nacionalidades de um dataset\"\"\"\n",
    "    if df is None:\n",
    "        print(f\"‚ùå Dataset {dataset_name} n√£o dispon√≠vel\")\n",
    "        return None\n",
    "    \n",
    "    # Identificar coluna de nacionalidade (geralmente primeira coluna)\n",
    "    primeira_coluna = df.iloc[:, 0]\n",
    "    \n",
    "    # Filtrar linhas que parecem ser nacionalidades (excluir totais, cabe√ßalhos, etc.)\n",
    "    nacionalidades = primeira_coluna[primeira_coluna.notna()].tolist()\n",
    "    \n",
    "    # Filtrar valores que n√£o s√£o nacionalidades\n",
    "    filtros_exclusao = ['total', 'popula√ß√£o', 'quadro', 'nacionalidade', 'fonte', 'ine']\n",
    "    \n",
    "    nacionalidades_filtradas = [\n",
    "        nac for nac in nacionalidades \n",
    "        if not any(filtro in str(nac).lower() for filtro in filtros_exclusao)\n",
    "        and len(str(nac)) > 2\n",
    "    ]\n",
    "    \n",
    "    print(f\"\\nüìä {dataset_name}:\")\n",
    "    print(f\"   ‚Ä¢ Total de entradas: {len(nacionalidades)}\")\n",
    "    print(f\"   ‚Ä¢ Nacionalidades identificadas: {len(nacionalidades_filtradas)}\")\n",
    "    \n",
    "    if len(nacionalidades_filtradas) > 0:\n",
    "        print(f\"   ‚Ä¢ Top 10 nacionalidades:\")\n",
    "        for i, nac in enumerate(nacionalidades_filtradas[:10], 1):\n",
    "            print(f\"     {i:2d}. {nac}\")\n",
    "    \n",
    "    return nacionalidades_filtradas\n",
    "\n",
    "# An√°lise por dataset\n",
    "nac_educacao = extract_nacionalidades(df_educacao, \"Habilita√ß√µes Liter√°rias\")\n",
    "nac_setores = extract_nacionalidades(df_setores, \"Setores de Atividade\") \n",
    "nac_demografia = extract_nacionalidades(df_demografia, \"Demografia\")\n",
    "\n",
    "# Consolida√ß√£o das nacionalidades\n",
    "all_nacionalidades = set()\n",
    "if nac_educacao:\n",
    "    all_nacionalidades.update(nac_educacao)\n",
    "if nac_setores:\n",
    "    all_nacionalidades.update(nac_setores)\n",
    "if nac_demografia:\n",
    "    all_nacionalidades.update(nac_demografia)\n",
